{
  "name": "cfpb-design-system",
  "version": "0.15.1",
  "private": true,
  "description": "CFPB's UI framework",
  "keywords": [
    "design-system"
  ],
  "bugs": {
    "url": "https://github.com/cfpb/design-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cfpb/design-system.git"
  },
  "license": "SEE LICENSE IN TERMS.md",
  "author": {
    "name": "Consumer Financial Protection Bureau",
    "email": "tech@cfpb.gov",
    "url": "https://cfpb.github.io/"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "a11y": "lhci autorun",
    "build": "yarn run compile-dev && bundle exec jekyll build",
    "build-netlify": "NODE_VERSION=14.17.5 && yarn && yarn run compile-prod && bundle exec jekyll build",
    "changelog": "./scripts/generate-changelog.sh",
    "compile-prod": "webpack --config webpack.config.docs.js --mode=production",
    "compile-dev": "webpack --config webpack.config.docs.js --mode=development",
    "copy-icons": "cp -r packages/cfpb-icons/src/icons docs/_includes/",
    "design-system-link": "lerna exec -- yarn link",
    "design-system-unlink": "lerna exec -- yarn unlink",
    "install": "yarn run copy-icons && bundle install",
    "process-icon-svgs": "svgo -f packages/cfpb-icons/src/icons --config=svgo.config.js",
    "release": "yarn run build && gulp build && lerna publish --no-verify-access",
    "start": "yarn run compile-dev && concurrently --kill-others \"yarn serve-netlify\" \"yarn serve-jekyll\" \"yarn run compile-dev --watch\"",
    "serve-html": "http-server docs/_site",
    "serve-jekyll": "bundle exec jekyll serve --watch --profile --host=localhost --port=4000",
    "serve-netlify": "netlify-cms-proxy-server",
    "test": "gulp test",
    "test:browser": "wdio run ./wdio.config.js",
    "test:browser:cloud": ": ${SAUCE_USERNAME?\"You need to define SAUCE_USERNAME and SAUCE_ACCESS_KEY env variables \"} && wdio run ./wdio.config.sauce.js",
    "version": "yarn run changelog && git add CHANGELOG.md"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead, ie >= 11",
  "changelog": {
    "labels": {
      "lerna-changelog/breaking": "Breaking Change",
      "lerna-changelog/enhancement": "Enhancement",
      "lerna-changelog/bug": "Bug Fix",
      "lerna-changelog/documentation": "Documentation",
      "lerna-changelog/internal": "Internal"
    }
  },
  "dependencies": {
    "anchor-js": "4.3.1",
    "core-js": "3.22.2",
    "ftdomdelegate": "4.0.6",
    "html-entities": "2.3.3",
    "lunr": "2.3.9",
    "marked": "4.0.14",
    "netlify-cms": "2.10.192",
    "normalize-css": "^2.3.1",
    "react": "17.0.2",
    "react-liquid": "2.0.1",
    "regenerator-runtime": "0.13.9",
    "slugify": "1.6.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/plugin-syntax-export-extensions": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "7.16.7",
    "@lhci/cli": "^0.9.0",
    "@wdio/cli": "7.19.5",
    "@wdio/local-runner": "7.19.5",
    "@wdio/mocha-framework": "7.19.5",
    "@wdio/sauce-service": "7.19.5",
    "@wdio/spec-reporter": "7.19.5",
    "autoprefixer": "10.4.4",
    "babel-loader": "8.2.5",
    "chromedriver": "100.0.0",
    "concurrently": "7.1.0",
    "css-loader": "6.7.1",
    "cssnano": "5.1.7",
    "escape-html": "^1.0.3",
    "fancy-log": "2.0.0",
    "finalhandler": "1.2.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "6.0.0",
    "gulp-ignore": "3.0.0",
    "gulp-less": "5.0.0",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-stylelint": "13.0.0",
    "html-loader": "3.1.0",
    "http-server": "14.1.0",
    "jest": "27.5.1",
    "jest-cli": "27.5.1",
    "lerna": "4.0.0",
    "lerna-changelog": "2.2.0",
    "less-loader": "10.2.0",
    "log-symbols": "5.1.0",
    "merge-stream": "2.0.0",
    "mini-css-extract-plugin": "2.6.0",
    "minimist": "1.2.6",
    "netlify-cms-proxy-server": "1.3.24",
    "postcss": "8.4.12",
    "postcss-less": "^6.0.0",
    "postcss-loader": "6.2.1",
    "require-dir": "^1.2.0",
    "serve-static": "1.15.0",
    "style-loader": "3.3.1",
    "stylelint": "^14.7.1",
    "stylelint-config-recommended-less": "^1.0.4",
    "svg-inline-loader": "0.8.2",
    "svgo": "2.8.0",
    "terser-webpack-plugin": "5.3.1",
    "through2": "4.0.2",
    "vinyl-named": "^1.1.0",
    "wcag": "^0.3.0",
    "wdio-chromedriver-service": "7.3.2",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-stream": "7.0.0"
  }
}
